{
  "name": "sherlock",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "typings install",
    "ts": "gulp ts",
    "ts:clean": "gulp ts:clean",
    "minify": "uglifyjs $npm_package_bundle --output $npm_package_bundlemin --source-map $npm_package_bundlemap --prefix relative --mangle --compress",
    "minify:clean": "trash $npm_package_bundlemin && trash $npm_package_bundlemap",
    "style": "gulp sass",
    "style:clean": "gulp sass:clean",
    "html": "gulp html",
    "html:clean": "gulp html clean",
    "build:dev": "npm run ts && npm run bundle && npm run style && npm run html",
    "build": "npm run ts && npm run bundle && npm run minify && npm run style && npm run html",
    "build:clean": "npm run ts:clean && trash build && npm run html:clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "http-server -a 0.0.0.0 -p 9000"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "browserify": "^13.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "html-injector": "2.1.0",
    "html-minifier": "^2.1.3",
    "http-server": "^0.9.0",
    "node-sass": "^3.7.0",
    "postcss-cli": "^2.5.2",
    "trash": "^3.4.1",
    "trash-cli": "^1.3.0",
    "tsify": "^0.16.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.5",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "angular": "^1.5.6",
    "angular-ui-router": "^0.3.1",
    "pouchdb": "^5.4.1"
  }
}
