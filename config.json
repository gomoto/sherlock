{
  "dist": {
    "html": "index.html",
    "css": "app.css",
    "js": "app.js"
  },
  "entry": {
    "html": "src/app.html",
    "scss": "src/app.scss",
    "ts": "src/app.ts"
  },
  "src": {
    "html": "src/*/*.html",
    "scss": "src/**/*.scss",
    "ts": "src/**/*.ts"
  },
  "options": {
    "browserify": {
      "cache": {},
      "packageCache": {},
      "debug": true
    },
    "tsify": {
    },
    "sass": {
      "outputStyle": "compressed"
    },
    "autoprefixer": {
      "browsers": ["last 2 versions"]
    }
  },
  "vendor": {
    "default": {
      "angular": "node_modules/angular/angular.js",
      "angular-ui-router": "node_modules/angular-ui-router/release/angular-ui-router.js",
      "lodash": "node_modules/lodash/lodash.js",
      "ng-tags-input": "node_modules/ng-tags-input/build/ng-tags-input.js",
      "ng-tags-input-css": "node_modules/ng-tags-input/build/ng-tags-input.css",
      "pouchdb": "node_modules/pouchdb/dist/pouchdb.js"
    },
    "production": {
      "angular": "https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.6/angular.js",
      "angular-ui-router": "https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.1/angular-ui-router.js",
      "lodash": "https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.13.1/lodash.js",
      "ng-tags-input": "https://cdnjs.cloudflare.com/ajax/libs/ng-tags-input/3.1.1/ng-tags-input.js",
      "ng-tags-input-css": "https://cdnjs.cloudflare.com/ajax/libs/ng-tags-input/3.1.1/ng-tags-input.css",
      "pouchdb": "https://cdnjs.cloudflare.com/ajax/libs/pouchdb/5.4.1/pouchdb.js"
    }
  },
  "base": {
    "default": "/",
    "production": "sherlock/"
  }
}
