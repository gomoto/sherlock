{
  "compilerOptions": {
    "module": "commonjs",
    "noEmitOnError": false,
    "noImplicitAny": true,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "filesGlob": [
    "src/**/*.ts",
    "typings/**/*.d.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "src/app.routing.ts",
    "src/app.stormpath.ts",
    "src/app.ts",
    "src/brain/brain.controller.ts",
    "src/brain/brain.module.ts",
    "src/janitor/janitor.controller.ts",
    "src/janitor/janitor.module.ts",
    "src/navbar/navbar.module.ts",
    "src/note/note.controller.ts",
    "src/note/note.module.ts",
    "src/pouchdb/pouchdb.design.ts",
    "src/pouchdb/pouchdb.module.ts",
    "src/pouchdb/pouchdb.service.ts",
    "typings/globals/angular-ui-router/index.d.ts",
    "typings/globals/angular/index.d.ts",
    "typings/globals/jquery/index.d.ts",
    "typings/globals/lodash/index.d.ts",
    "typings/globals/node/index.d.ts",
    "typings/index.d.ts",
    "typings/pouchdb/pouchdb.d.ts"
  ]
}
